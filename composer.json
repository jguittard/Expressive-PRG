    "description": "Middleware to implement the Post/Redirect/Get pattern in Zend Expressive",
    "type": "library",
    "keywords": [
        "zend",
        "expressive",
        "netglue",
        "prg"
    ],
    "homepage": "https://github.com/netglue/Expressive-PRG",
    "require": {
        "zendframework/zend-expressive": "^1.0",
        "zendframework/zend-session": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "George Steel",
            "email": "george@net-glue.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "NetglueExpressive\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NetglueExpressiveTest\\": "test/NetglueExpressiveTest"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
